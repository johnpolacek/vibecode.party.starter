{
  "name": "vibecode-party-starter",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p ${PORT:-3000}",
    "dev:db": "open -a Docker && sleep 5 && pnpm db:start && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "go": "git add . && aicommits && git push origin main",
    "go:db": "supabase db lint && supabase db push --debug --include-all && pnpm go",
    "test": "playwright test",
    "pw": "playwright test --ui --debug",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:admin": "open http://127.0.0.1:55326/project/default",
    "db:lint:local": "supabase db lint --db-url 'postgresql://postgres:postgres@127.0.0.1:55324/postgres' --level warning",
    "db:push:local": "supabase db push --db-url=postgresql://postgres:postgres@localhost:55324/postgres --include-all",
    "db:reset:local": "supabase db reset --db-url=postgresql://postgres:postgres@localhost:55324/postgres",
    "db:push:prod": "supabase db push --debug --include-all",
    "db:diff": "supabase db diff --use-migra -f migration",
    "ship": "pnpm lint && pnpm build && (pnpm dev & sleep 10 && pnpm test && kill $!) && supabase db lint && supabase db push --debug --include-all && pnpm go"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.772.0",
    "@aws-sdk/s3-request-presigner": "^3.772.0",
    "@clerk/backend": "^1.25.5",
    "@clerk/nextjs": "^6.12.8",
    "@heroicons/react": "^2.2.0",
    "@octokit/rest": "^21.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@sendgrid/mail": "^8.1.4",
    "@stripe/react-stripe-js": "^3.5.1",
    "@stripe/stripe-js": "^6.1.0",
    "@supabase/supabase-js": "^2.49.1",
    "@types/react-google-recaptcha": "^2.1.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "lucide-react": "^0.483.0",
    "next": "15.2.3",
    "next-themes": "^0.4.6",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "slugify": "^1.6.6",
    "sonner": "^2.0.1",
    "stripe": "^17.7.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.3",
    "supabase": "^2.19.7",
    "tailwindcss": "^4.0.15",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
